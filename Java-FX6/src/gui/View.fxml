      <Button fx:id="btAll" layoutX="164.0" layoutY="36.0" mnemonicParsing="false" onAction="#onBtAllAction" text="ALL" AnchorPane.topAnchor="10.0" />
      <ListView fx:id="listViewShow" layoutX="13.0" layoutY="45.0" onEditStart="#onBtAllAction" prefHeight="95.0" prefWidth="184.0" AnchorPane.leftAnchor="13.0" />
   </children>
</AnchorPane>
